{
    "manifest_version": 2,
    "name": "Twitter extraction",
    "version": "1.0",
    "description": "Extract tweets from a user profile/thread",
    "icons": {
        "48": "icons/icon-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://twitter.com/*"
            ],
            "js": [
                "twitter.js"
            ]
        }
    ]
}